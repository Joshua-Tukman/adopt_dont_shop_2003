<%= form_tag "/pets/#{@pet.id}", method: :patch do %>

  <p><%= label_tag "image" %></p><br>
  <p><%= text_field_tag :image %></p><br>
  <p><%= label_tag "Name" %></p><br>
  <p><%= text_field_tag :name %></p><br>
  <p><%= label_tag "Description" %></p><br>
  <p><%= text_field_tag :description %></p><br>
  <p><%= label_tag "Age" %></p><br>
  <p><%= text_field_tag :age %></p><br>
  <p><%= label_tag "Sex" %></p><br>
  <p><%= text_field_tag :sex %></p><br>
  <p><%= submit_tag "Update Pet" %>
<% end %>
